CREATE TABLE users (
  user_id SERIAL PRIMARY KEY,
  user_name VARCHAR(255) NOT NULL,
  email VARCHAR(255) NULL,
  password INT UNIQUE
);


INSERT INTO users (user_id, user_name, email, password) VALUES ( 0,'Tuti', 'bb@gmail.com',424789);
INSERT INTO users (user_id, user_name, email, password) VALUES ( 1,'Enanaye','her@gmail.com',678901);
INSERT INTO users (user_id, user_name, email, password) VALUES ( 2,'Asheeta','ashee@gmail.com',213456);



CREATE TABLE mesages (
  user_id SERIAL PRIMARY KEY,
  user_message NOT NULL
);


INSERT INTO messages (user_id, user_message) VALUES ( 0, 'I am a Princess!');
